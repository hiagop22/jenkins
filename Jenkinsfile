pipeline {
    // posso usar qualquer pipeline
    // o agente pode ser container, cluster,
    // docker, pod cubernet.
    // como é uma pipe básica, pode ser qualquer agente
    agent any

    // Posso ter: build, test, deploy e por aí vai
    stages{
        stage ("Inicial") {
            steps{
                echo 'Iniciando a pipeline 0'
            }
        }
    }
}